{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check --ignore-path .eslintignore .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write --ignore-path .eslintignore . && eslint --output-file eslint_report.json --format json . --ext ts --ext tsx --ext tsx --fix && git add ."
  },
  "pre-commit": [
    "check-lint",
    "check-format"
  ],
  "optionalDependencies": {
    "@swc/core-darwin-arm64": "$@swc/core",
    "@swc/core-darwin-x64": "$@swc/core",
    "@swc/core-linux-x64-gnu": "$@swc/core"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom",
    "@types/react": "$@types/react"
  },
  "dependencies": {
    "@emotion/css": "11.10.5",
    "@emotion/react": "11.10.6",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.5",
    "@hookform/resolvers": "2.9.11",
    "@mui/lab": "5.0.0-alpha.119",
    "@mui/material": "5.11.8",
    "@mui/styles": "5.11.7",
    "@mui/x-date-pickers": "5.0.19",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@uiw/react-md-editor": "3.20.5",
    "@walletconnect/web3-provider": "1.8.0",
    "axios": "1.3.2",
    "classnames": "2.3.2",
    "date-fns": "2.29.3",
    "emotion": "11.0.0",
    "emotion-server": "11.0.0",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "8.5.0",
    "i18next-browser-languagedetector": "7.0.1",
    "jest-environment-jsdom": "28.1.3",
    "lodash.merge": "4.6.2",
    "nanoid": "4.0.0",
    "next": "13.2.4",
    "next-language-detector": "1.0.2",
    "next-redux-wrapper": "8.0.0",
    "next-remove-imports": "1.0.8",
    "prettier": "2.8.3",
    "react": "18.2.0",
    "react-blockies": "1.4.1",
    "react-canvas-confetti": "1.3.0",
    "react-countdown": "2.3.5",
    "react-csv": "2.2.2",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.1",
    "react-hook-form-mui": "5.14.0",
    "react-redux": "8.0.5",
    "react-toastify": "9.1.1",
    "redux": "4.2.0",
    "redux-devtools-extension": "2.13.9",
    "redux-thunk": "2.4.1",
    "typescript": "4.9.5",
    "zod": "3.20.5"
  },
  "devDependencies": {
    "@iconify/react": "4.1.0",
    "@next/eslint-plugin-next": "13.0.2",
    "@swc/core": "1.3.40",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@types/css": "0.0.33",
    "@types/express": "4.17.17",
    "@types/lodash.merge": "4.6.7",
    "@types/node": "18.15.0",
    "@types/react": "18.0.28",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/parser": "5.33.0",
    "autoprefixer": "10.4.14",
    "babel-plugin-direct-import": "1.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.29.0",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "pre-commit": "1.2.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2"
  }
}
